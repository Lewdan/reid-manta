{
    "model" : {
        "type":                 "TripletLoss",
        "backend":              "InceptionV3",
        "connect_layer":        -1,
        "precomp_feat":         false,
        "frontend":             "glob_pool",
        "train_from_layer":     0,
        "input_width":          448,
        "input_height":         224,
        "_dropout_p":           0.3,
        "embedding_size":       256,
        "loss":                 "semi_hard_triplet"
    },

    "data": {
        "train_image_folder":   "examples/feralcat/localized/data/",
        "split_seed":            777

    },

    "train": {
        "pretrained_weights":   "",
        "batch_size":           12,
        "learning_rate":        1e-5,
        "nb_epochs":            400,
        "log_step":             10,
        "distance":             "l2",
        "exp_dir":              "experiments",
        "exp_id":               "feral-cat",
        "debug":                false,
        "aug_rate":            "cat",
        "cl_per_batch":        15,
        "sampl_per_class":     5,
        "equal_k":             true,
        "verbose":             1
    },

    "evaluate": {
        "accuracy_at_k":        [1, 5, 10],
        "move_to_dataset":      2,
        "test_set":             "",
        "test_size":             0.4,
        "n_eval_epochs":        20,
        "far_target":           0.01

    },

    "general": {
        "stdout-file":          true

    },

    "prod": {
        "prefix":              "feralcat-db",
        "output":              "examples/feralcat/localized/data",
        "lfile":               "",
        "embeddings":          "examples/feralcat/db_embs",
        "temp":                "examples/feralcat"

    }
}
